<?xml version="1.0" encoding="UTF-8" ?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <sampleQuery><![CDATA[select * from {table} where xmlstring="<example><a>A</a><b>B</b></example>"]]></sampleQuery>
    <description>Parse an XML string</description>
    <author>Peter Cowburn</author>
  </meta>
  <bindings>
    <select itemPath="" produces="XML">
      <inputs>
        <key id="xmlstring" type="xs:string" paramType="variable" required="true"/>
      </inputs>
      <execute><![CDATA[
response.object = new XML(xmlstring);
]]></execute>
    </select>
  </bindings>
</table>
